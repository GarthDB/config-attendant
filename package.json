{
  "name": "config-attendant",
  "description": "rc + package.json config data + custom data",
  "main": "./lib",
  "browserify": "./lib/browser.js",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d ./ ./src/",
    "test": "nyc ava",
    "lint": "eslint .",
    "report-coverage": "nyc report -r lcovonly && codecov",
    "checkdocs": "atomdoc src",
    "validate": "npm run lint && npm run test && npm run checkdocs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/config-attendant.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "config",
    "rc",
    "unix",
    "defaults",
    "package.json"
  ],
  "bin": {
    "config": "./bin/index.js"
  },
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "contributors": [
    "Dominic Tarr <dominic.tarr@gmail.com> (dominictarr.com)"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "deep-extend": "^0.5.1",
    "ini": "^1.3.5",
    "minimist": "^1.2.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "atomdoc-cli": "^1.1.1",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "codecov": "^3.0.2",
    "eslint": "^4.4.1",
    "nixt": "^0.5.0",
    "nyc": "^11.1.0"
  },
  "version": "0.1.1"
}
